{% extends "_base.console.twig" %}

{% set composed = true %}

{% block type %}
    {% if var.title is not empty %}
        <t_array_block>{{ var.title|tags }}</t_array_block><intro>
    {% endif %}
{% endblock %}

{% block value %}
    {{ m.indent(var.level + 1) }}Path:<space>file://{{ var.tempPath }}<intro>
    {{ m.indent(var.level + 1) }}Dimensions:<space>{{ var.width }}x{{ var.height }}
{% endblock %}
