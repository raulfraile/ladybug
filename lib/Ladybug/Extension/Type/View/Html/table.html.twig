{% extends "_base.html.twig" %}

{% block value %}

    <div style="overflow-x: scroll;overflow-y: scroll;height: 224px;">
        <table class="table table-striped table-bordered">

            <thead>
                <tr>
                    {% for item in var.header %}
                        <th>{{ item }}</th>
                    {% endfor %}
                </tr>
            </thead>

            <tbody>
                {% for item in var.data %}
                    <tr>
                        {% for subitem in item %}
                            <td>{{ subitem }}</td>
                        {% endfor %}
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>


{% endblock %}
